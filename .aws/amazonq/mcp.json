{
  "mcpServers": {
    "vercel": {
      "url": "https://mcp.vercel.com",
      "type": "http"
    },
    "aws-knowledge": {
      "url": "https://knowledge-mcp.global.api.aws",
      "type": "http"
    },
    "fetch": {
      "command": "uvx",
      "args": ["mcp-server-fetch"]
    },
    "strands-agents": {
      "command": "uvx",
      "args": ["strands-agents-mcp-server"],
      "autoApprove": ["search_docs", "fetch_doc"]
    }
  }
}
